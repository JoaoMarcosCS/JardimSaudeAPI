{
  "version": 2,
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "(.*)",
      "dest": "src/server.ts",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE"
      ],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ],
  "cors": {
    "origins": [
      "http://localhost:3000",
      "https://bug-free-system-qgvxv594765c97rx-3000.app.github.dev",
      "https://jardim-saude.vercel.app/"
    ]
  }
}